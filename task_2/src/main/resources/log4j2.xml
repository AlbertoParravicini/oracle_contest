<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss,SSS} %p %C{1} - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="error">
			<AppenderRef ref="Console"/>
		</Root>
		<Logger name="oracle.pgx.engine.admin.Ctrl" level="info">
			<AppenderRef ref="Console"/>
		</Logger>
		<Logger name="pgx.dist.cluster_host" level="info">
			<AppenderRef ref="Console"/>
		</Logger>
		<Logger name="org.deeplearning4j" level="off">
			<AppenderRef ref="Console"/>
		</Logger>
		<Logger name="org.reflections" level="off">
			<AppenderRef ref="Console"/>
		</Logger>
	</Loggers>
</Configuration>